#include <iostream>
#include <string>

using namespace std;

int main()
{
    int key, tmp_key = 0;
    string input;
    cin>>input;
    cin>>key;

    for (ulong i = 0; i < input.length(); ++i)
    {
	if(isdigit(input[i]))
	{
	    tmp_key = key % 10;
	    input[i] += tmp_key;
	    if (input[i] >= 58 && input[i] <= 64)
		input[i] -= 10;
	}
	if(input[i] >= 65 && input[i] <= 90)
	{
	    tmp_key = key % 26;
	    input[i] += tmp_key;
	    if(input[i] > 90 && input[i] <= 96)
		input[i] -= 26;
	}
	if (input[i] >= 97 && input[i] <= 122)
	{
	    tmp_key = key % 26;
	    cout<<"THIS IS IT IN INT BEFORE -> " << int(input[i])<<endl;

	    // This is the problem case
	    // abcdZXYzxy-999.@
	    // 	200

	    input[i] += tmp_key;
	    if(input[i] > 122)
		input[i] -= 26;
	    cout<<"THIS IS IT IN INT -> " << int(input[i])<<endl;
	}
	cout<<input[i];
    }
    cout<<"\n";

    return 0;
}
